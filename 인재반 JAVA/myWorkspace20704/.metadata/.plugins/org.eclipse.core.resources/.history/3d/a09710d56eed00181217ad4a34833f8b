package day1112;
/*
 * 콘솔창에는 1초 카운트가 되고있고
 * 구구단 문제가 jop로 계속 나오고 있고
 * 구구단을 5문제 맞히면 경과된 초를 출력
 */
class Count2 extends Thread{
	@Override
	public void run() {
		for(int i=1;i>0;i++) {
			System.out.println(i+"초");
			try {
				Thread.sleep(1000);
			} catch (InterruptedException e) {
			
			}
		}
	}
}//end of Count2 class
public class Quiz09 {//main class

	public static void main(String[] args) {
		
		
	}//end of main method

}//end of main class
